class Bicycle{ 

//Data Member

	private String ownerName;

//Constructor Initialize the Date Members

	public Bicycle(){

		ownerName = "unknown";

	}
//Returns the name of the Bicycle's owner

	public String getownerName(){

		return ownerName;

	}
	//Assign the name of the Bicycle owner

	public void SetOwnerName(String name){

		ownerName = name;
	}


	}



class BicycleRegistration
{

	public static void main(String[] args){

		Bicycle bike1, bike2; 
		String  owner1, owner2;

	    bike1 = new Bicycle(); //Create and assign Value to Bike1 
		bike1.setOwnerName("Adam Smith");

		bike2 = new Bicycle(); //Create and assign Value to Bike2
		bike2.setOwnerName("Ben Jones");

		//out the information

		owner1 = bike1.getOwnerName();
		owner2 = bike2.getOwnerName();

		System.out.println(owner1 + " owns the Bicycle");
		System.out.println(owner2 + " also owns a Bicycle");

	}
}

